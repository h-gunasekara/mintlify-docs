{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Catalog API",
  "logo": {
    "dark": "/logo/catalog-logo-white.svg",
    "light": "/logo/catalog-logo-black.svg"
  },
  "favicon": "/logo/catalog-logo-black.svg",
  "colors": {
    "primary": "#00B158",
    "light": "#00D166",
    "dark": "#008F47",
    "anchors": {
      "from": "#00B158",
      "to": "#00D166"
    }
  },
  "topbar": {
    "style": "gradient"
  },
  "topbarCtaButton": {
    "name": "Get API Key",
    "url": "mailto:founders@getcatalog.ai"
  },
  "anchors": [
    {
      "name": "Website",
      "icon": "globe",
      "url": "https://www.getcatalog.ai"
    },
    {
      "name": "Support",
      "icon": "envelope",
      "url": "mailto:founders@getcatalog.ai"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": [
        "welcome",
        "quickstart",
        "authentication"
      ]
    },
    {
      "group": "API Reference",
      "pages": [
        "api-reference/overview",
        {
          "group": "Search",
          "pages": [
            "api-reference/endpoints/search/search-products"
          ]
        },
        {
          "group": "Feed",
          "pages": [
            "api-reference/endpoints/feed/get-feed"
          ]
        },
        {
          "group": "Product",
          "pages": [
            "api-reference/endpoints/products/get-product"
          ]
        },
        {
          "group": "Vendors",
          "pages": [
            "api-reference/endpoints/vendors/list-vendors"
          ]
        }
      ]
    },
    {
      "group": "Guides",
      "pages": [
        "guides/filtering",
        "guides/pagination",
        "guides/error-handling"
      ]
    }
  ],

  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude"
    ]
  }
} 